{
  "name": "fourier-making-waves",
  "version": "1.0.19",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/fourier-making-waves.git"
  },
  "devDependencies": {
    "grunt": "~1.4.0"
  },
  "phet": {
    "requirejsNamespace": "FOURIER_MAKING_WAVES",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "phet-io",
      "adapted-from-phet"
    ],
    "phetLibs": [
      "bamboo",
      "twixt",
      "vegas"
    ],
    "phet-io": {
      "validation": false
    },
    "simFeatures": {
      "supportsSound": true,
      "supportsInteractiveDescription": true
    },
    "supportsOutputJS": true,
    "screenNameKeys": [
      "FOURIER_MAKING_WAVES/screen.discrete",
      "FOURIER_MAKING_WAVES/screen.waveGame",
      "FOURIER_MAKING_WAVES/screen.wavePacket"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js"
  }
}